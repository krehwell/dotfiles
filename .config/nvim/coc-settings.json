{
  "coc.preferences.formatOnSaveFiletypes": ["css", "markdown", "json", "html"],
  "prettier.printWidth": 160,
  "tsserver.npm": "/usr/bin/npm",
  "eslint.autoFixOnSave": false,
  "diagnostic.checkCurrentLine": true,
  "eslint.enable": true,
  "eslint.nodePath": "/usr/local/lib/node_modules/",
  "eslint.filetypes": ["javascript", "typescript", "typescriptreact", "javascriptreact"],
  "eslint.options": {
    "env": {
      "browser": true,
      "node": true
    }
  },
  "tsserver.reportStyleChecksAsWarnings": true,
  "pairs.enableCharacters": ["(", "[", "{", "<"]
}
